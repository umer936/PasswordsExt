{
  "manifest_version": 2,
  "name": "Password Requirement Checker",
  "description": "Because no one can ever remember what the site wants",
  "version": "0.0.65",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "I need help"
  },
  "permissions": [
    "activeTab"
  ]
}